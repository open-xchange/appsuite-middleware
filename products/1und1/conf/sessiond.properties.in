#
# sessiond.properties
#

# start a sessiond socket
com.openexchange.sessiond.isServerSocketEnabled:false

# start a sessiond object stream socket
com.openexchange.sessiond.isServerObjectStreamSocketEnabled:false

# sessiond socket on this port
com.openexchange.sessiond.serverPort:@sessiondport@

# sessiond socket on this object stream port
com.openexchange.sessiond.serverObjectStreamPort:33335

# enable communication between client and server over tcp
com.openexchange.sessiond.isTcpClientSocketEnabled:false

# session container timeout
com.openexchange.sessiond.sessionContainerTimeout:4200000
	
# internal session container
com.openexchange.sessiond.numberOfSessionContainers:8

# max sessions in sessiond
com.openexchange.sessiond.maxSession:5000

# accept only sockets from this address
com.openexchange.sessiond.serverBindAddress:localhost

# check if a user already has a session and overwrite it
com.openexchange.sessiond.isDoubleLoginPermitted:true
	
# the user who can request and kick sessions
com.openexchange.sessiond.sessionAuthUser:mailadmin

# use secure socket connection
com.openexchange.sessiond.isSecureSocketConnectionEnabled:false

# value in milliseconds
com.openexchange.sessiond.sessionDefaultLifeTime:3600000

# CA File
com.openexchange.sessiond.caFile:

# Cert File
com.openexchange.sessiond.certFile:

# Key File
com.openexchange.sessiond.keyFile:
